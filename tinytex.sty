%%% TIKZ
\long\def\ifnodedefined#1#2#3{%
	\@ifundefined{pgf@sh@ns@#1}{#3}{#2}%
}
\newcommand{\tikzeq}[2]{\tikzmark{#1__start}#2\tikzmark{#1__end}}
\newcommand{\tikzequnderbrace}[2] {
	\draw[decorate, decoration={brace,mirror,raise=0.5em}] (pic cs:#1__start) -- (pic cs:#1__end);
	\node [anchor=north, yshift=-0.75em] (#1__node) at ($(pic cs:#1__start)!0.5!(pic cs:#1__end)$) {\small {#2}};
}
\newcommand{\tikzequnderbracemore}[2] {
\draw[decorate, decoration={brace,mirror,raise=0.8em}] (pic cs:#1__start) -- (pic cs:#1__end);
\node [anchor=north, yshift=-1.15em] (#1__node) at ($(pic cs:#1__start)!0.5!(pic cs:#1__end)$) {\small {#2}};
}
\newcommand{\tikzeqoverbrace}[2] {
	\draw[decorate, decoration={brace,raise=0.9em}] (pic cs:#1__start) -- (pic cs:#1__end);
	\node [above, anchor=north, yshift=2.3em] (#1__node) at ($(pic cs:#1__start)!0.5!(pic cs:#1__end)$) {\small {#2}};
}
\newcommand{\tikzeqoverbracemore}[2] {
\draw[decorate, decoration={brace,raise=1.2em}] (pic cs:#1__start) -- (pic cs:#1__end);
\node [above, anchor=north, yshift=2.7em] (#1__node) at ($(pic cs:#1__start)!0.5!(pic cs:#1__end)$) {\small {#2}};
}
\newcommand{\tikzeqrightbrace}[2] {
\draw[decorate, decoration={brace,mirror,raise=0.0em}] (#1__node.south east) -- 
	(#1__node.north east);
\node [anchor=west, xshift=0.2em] at (#1__node.east) {\small #2};
}
\newcommand{\tikzeqleftbrace}[2] {
\draw[decorate, decoration={brace,raise=0.0em}] (#1__node.south west) -- 
(#1__node.north west);
\node [anchor=east, xshift=0em] at (#1__node.west) {\small #2};
}

% 1: tikzeq name
% 2: height
% 3: new node name
\newcommand{\emphnode}[4] {
	\node [anchor=center, yshift=0.2em, fit=(pic cs:#1__start)(pic cs:#1__end),emph, minimum height=#2, draw=rossoPantano] (#3) {};
}
\newcommand{\emphedge}[4] {
	\draw[->] (#1) edge [out=#3, in=#4, emph] (#2);
}
